{
  "name": "ng-idg",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "node server.js",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "heroku-postbuild": "ng build --aot --prod",
    "conn:build": "ng build idg-connect && npm i dist/idg-connect --save-optional",
    "mat:build": "ng build idg-mat && npm i dist/idg-mat --save-optional",
    "idg:build": "npm run conn:build && npm run mat:build",
    "conn:build:prod": "ng build idg-connect --prod && npm i dist/idg-connect --save-optional",
    "mat:build:prod": "ng build idg-mat --prod && npm i dist/idg-mat --save-optional",
    "idg:build:prod": "npm run conn:build:prod && npm run mat:build:prod"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.0.2",
    "@angular/cdk": "^11.0.0",
    "@angular/cli": "~11.0.2",
    "@angular/common": "~11.0.2",
    "@angular/compiler": "~11.0.2",
    "@angular/compiler-cli": "~11.0.2",
    "@angular/core": "~11.0.2",
    "@angular/flex-layout": "^9.0.0-beta.31",
    "@angular/forms": "~11.0.2",
    "@angular/material": "^11.0.0",
    "@angular/platform-browser": "~11.0.2",
    "@angular/platform-browser-dynamic": "~11.0.2",
    "@angular/router": "~11.0.2",
    "express": "^4.17.1",
    "path": "^0.12.7",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.2",
    "@angular/language-service": "~11.0.2",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.19.6",
    "codelyzer": "^6.0.0",
    "enhanced-resolve": "^3.4.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.4",
    "ng-packagr": "^11.0.0",
    "protractor": "~7.0.0",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.3",
    "typescript": "~4.0.5"
  },
  "optionalDependencies": {
    "@itia-digital/connect": "file:dist/idg-connect",
    "@itia-digital/material": "file:dist/idg-mat"
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 120,
    "tabWidth": 4
  },
  "engines": {
    "node": "v13.10.1",
    "npm": "6.13.7"
  }
}
